{
  "name": "test",
  "version": "1.0.0-SNAPSHOT",
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "clean:cucumber": "rimraf cypress/cucumber-json reports",
    "clean:cypress": "rimraf cypress/screenshots cypress/videos cypress/results cypress/report cypress/downloads",
    "clean:test-results": "rimraf test-results",
    "clean:ngcache": "ng cache clean",
    "clean:generated_features": "rimraf cypress/features/generated_features",
    "clean:logs": "rimraf logs",
    "clean": "run-p clean:dist clean:coverage clean:cucumber clean:cypress clean:test-results clean:ngcache clean:generated_features clean:logs",
    "ng": "ng",
    "prestart": "npm run clean:dist",
    "start": "ng serve",
    "prebuild": "npm run clean:dist",
    "build": "ng build",
    "build:dev": "npm run build -- -c development",
    "watch": "npm run build -- --watch --configuration development",
    "test": "jest",
    "e2e": "ng e2e",
    "precypress:run": "run-p clean:cypress clean:cucumber clean:generated_features",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:tags:happypath": "npm run cypress:run -- --env tags=\"@happypath\"",
    "cypress:run:tags:de": "npm run cypress:run -- --env tags=\"@DE\" || echo \"Oops, at least one cypress test failed!\"",
    "cypress:run:tags:en": "npm run cypress:run -- --env tags=\"@EN\" || echo \"Oops, at least one cypress test failed!\"",
    "postcypress:run": "npm run clean:generated_features && node cypress/utils/cucumber-html-report.js",
    "merge:mochawesome": "mochawesome-merge  cypress/results/*.json -o cypress/results/output.json",
    "generate:mochawesome:report": "npx marge cypress/results/output.json -f report -o cypress/report/mochawesome",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- --fix",
    "lint:ci": "npm run lint -- --silent=true --force=true --format=json > eslint-report.json",
    "lint:style": "stylelint \"./src/**/*.{css,scss}\"",
    "lint:style:fix": "npm run lint:style -- --fix",
    "lint:md": "markdownlint --ignore-path .gitignore .",
    "lint:md:fix": "npm run lint:md -- --fix",
    "lint:all": "npm-run-all lint lint:style lint:md",
    "pre-commit": "lint-staged"
  },
  "private": true,
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.5",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cucumber/gherkin": "^26.2.0",
    "@cucumber/gherkin-utils": "^8.0.5",
    "@cucumber/messages": "^22.0.0",
    "@deepakvishwakarma/cucumber-json-formatter": "^0.0.2",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/node": "^18.17.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "cy-verify-downloads": "^0.1.16",
    "cypress": "^12.17.3",
    "cypress-fail-fast": "^7.0.3",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-on-fix": "^1.0.2",
    "cypress-real-events": "^1.10.1",
    "cypress-xray-plugin": "^4.0.3",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "fs-extra": "11.1.1",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "multiple-cucumber-html-reporter": "3.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "rimraf": "5.0.1",
    "stream-promise": "^3.2.0",
    "typescript": "4.9.5"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "**/cypress/step_definitions/*.ts"
    ],
    "filterSpecs": true,
    "omitFiltered": true,
    "json": {
      "enabled": true,
      "output": "cypress/cucumber-json/formatted-report.json",
      "formatter": "node",
      "args": [
        "./node_modules/@deepakvishwakarma/cucumber-json-formatter/lib/main.js"
      ]
    },
    "messages": {
      "enabled": true,
      "output": "cypress/cucumber-json/cucumber-messages.ndjson"
    }
  }
}
